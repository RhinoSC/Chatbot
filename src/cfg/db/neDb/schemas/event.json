{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "_id": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "start": {
            "type": "number"
        },
        "end": {
            "type": "number"
        },
        "schedule": {
            "type": "object",
            "$ref": "./schedule.json"
        },
        "donations": {
            "type": "array",
            "default": [],
            "items": {
                "$ref": "./donation.json"
            }
        },
        "prizes": {
            "type": "array",
            "default": [],
            "items": {
                "$ref": "./prize.json"
            }
        }
    }
}