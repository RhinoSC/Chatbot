{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "_id": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "players": {
            "type": "array",
            "default": [],
            "items": {
                "$ref": "./user.json"
            }
        }
    },
    "required": [
        "_id",
        "name",
        "players"
    ]
}