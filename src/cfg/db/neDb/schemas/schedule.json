{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "_id": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "start": {
            "type": "number"
        },
        "end": {
            "type": "number"
        },
        "rows": {
            "type": "array",
            "default": [],
            "items": {
                "$ref": "./run.json"
            }
        }
    },
    "required": [
        "_id",
        "name",
        "start",
        "end",
        "rows"
    ]
}